<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.taylor.dao.OrderDao">
	<resultMap type="com.taylor.entity.Order" id="OrderResultMap">
		<result property="id" column="id" jdbcType="INTEGER" />
		<result property="merchantId" column="merchant_id" jdbcType="BIGINT" />
		<result property="name" column="name" jdbcType="VARCHAR" />
	</resultMap>
	<select id="queryOrderList" resultMap="OrderResultMap">
		select * from t_order
	</select>
</mapper>
